{
	"include": [
		"types"
	],
	"compilerOptions": {
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"newLine": "lf",
		"forceConsistentCasingInFileNames": true,
		"pretty": true,
		"target": "es5",
		"lib": [
			"dom",
			"es2015"
		],
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"baseUrl": "."
	}
}
